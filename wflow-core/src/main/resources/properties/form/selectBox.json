            value : 'true',
            label : ''
        }]
    },
    {
        name : 'readonlyLabel',
        label : '@@form.selectbox.readonlyLabel@@',
        type : 'checkbox',
        value : 'False',
        options : [{
            value : 'true',
            label : ''
        }]
    },
    {
        label : '@@form.selectbox.workflow@@',
        type : 'header'
    },
    {
        name : 'workflowVariable',
        label : '@@form.selectbox.workflowVariable@@',
        type : 'textfield'
    }]
