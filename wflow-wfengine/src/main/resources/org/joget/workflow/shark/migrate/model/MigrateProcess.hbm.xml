<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class entity-name="MigrateProcess" lazy="false" name="org.joget.workflow.shark.migrate.model.MigrateProcess" table="SHKProcesses">
        <id column="Id" name="id" type="string"/>
        <property column="ProcessDefinition" name="processDefinition" type="long"/>
        <property column="PDefName" name="name" type="string"/>
        <property column="State" name="state" type="long"/>   
        <property column="oid" name="oid" type="long"/>         
    </class>
</hibernate-mapping>



